{
  "name": "echarts-for-react",
  "version": "3.0.0-alpha.1",
  "description": "baidu Echarts(v3.x & v4.x) components for react.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "start": "cd demo && webpack-dev-server --watch",
    "lint": "eslint src && eslint __tests__ && eslint demo/src",
    "clean": "rimraf lib/core.js && rimraf lib/index.js",
    "test": "npm run lint && npm run cover",
    "demo": "cd demo && webpack",
    "build:demo": "cd demo && father-build --umd --file bar demo/src/index.js",
    "build": "father-build",
    "cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hustcc/echarts-for-react.git"
  },
  "keywords": [
    "react",
    "component",
    "echarts-react",
    "echarts",
    "react-echarts",
    "chart",
    "charts",
    "graph",
    "react-component"
  ],
  "author": "hustcc (https://github.com/hustcc), howel52 (https://github.com/howel52)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hustcc/echarts-for-react/issues"
  },
  "homepage": "https://github.com/hustcc/echarts-for-react",
  "devDependencies": {
    "76": "^0.0.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@types/echarts": "^4.4.1",
    "@types/react": "^16.9.15",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "echarts": "4.0.4",
    "echarts-gl": "^1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-typescript": "^0.14.0",
    "father-build": "^1.18.0",
    "file-loader": "^1.1.6",
    "jest": "^22.4.2",
    "jest-canvas-mock": "^2.0.0-alpha.0",
    "jest-electron": "^0.1.7",
    "jsx-loader": "^0.13.2",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^1.19.1",
    "react": "^15.5.0",
    "react-adsense": "^0.0.3",
    "react-dom": "^15.5.0",
    "react-router": "^2.0.0",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "style-loader": "^0.19.1",
    "typescript": "^3.7.3",
    "typescript-eslint-parser": "^22.0.0"

  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "fast-deep-equal": "^2.0.1",
    "size-sensor": "^0.2.0"
  },
  "peerDependencies": {
    "react": ">=0.13.2 || ^0.14 || ^15.0.0 || >=16.0.0",
    "prop-types": "^15.5.7",
    "echarts": "^3.0.0 || ^4.0.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*\\.spec)\\.(js|jsx)$",
    "testURL": "https://atool.vip/",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "./__tests__/setup.js",
      "jest-canvas-mock"
    ],
    "transform": {
      "^.+\\.js[x]?$": "babel-jest"
    },
    "moduleNameMapper": {},
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
